{
    "name":         "kupishoes/leos.partner.php-sdk-bundle",
    "type":         "symfony-bundle",
    "description":  "PHP-SDK SDK Bundle for interacting with Lamoda B2B Platform",
    "license":      "Private",
    "require":      {
        "symfony/framework-bundle": "2.*",
        "doctrine/orm": "*",
        "kupishoes/leos.partner.php-sdk": "~1.0.0"
    },
    "repositories": {
        "leos.partner.php-sdk": {
            "type": "vcs",
            "url": "git@github.com:pavelsavelyev/lamoda-b2b-platform.php-sdk.git"
        },
        "leos.partner.dto": {
            "type": "vcs",
            "url": "git@github.com:pavelsavelyev/lamoda-b2b-platform.dto.git"
        }
    },
    "autoload":     {
        "psr-0": {
            "LamodaB2B\\Bundle\\LamodaB2BBundle": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir":   "LamodaB2B/Bundle/LamodaB2BBundle"
}
